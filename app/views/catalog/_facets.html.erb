<% # main container for facets/limits menu -%>
<% if has_facet_values? facet_field_names, @response %>

  <% if params['controller'] == "press_catalog" %>
    <%= render 'user_access_fake_facet' %>
  <% end %>
  <% # container for facet groups -%>
  <% Deprecation.silence(Blacklight::Facet) { facet_group_names }.each do |groupname| %>
    <%= render 'facet_group', groupname: groupname %>
  <% end %>
  <% if params['controller'] == "press_catalog" %>
    <%= render 'access_icon_legend' %>
  <% end %>
<% end %>
