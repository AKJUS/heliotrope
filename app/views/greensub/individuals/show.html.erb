<p id="notice"><%= notice %></p>
<div id="maincontent">
  <h1>Individual</h1>
  <div class="col-md-1"><em>ID</em></div>
  <div class="col-md-11">&nbsp;<%= @individual.id %></div>
  <div class="col-md-1"><em>Identifier</em></div>
  <div class="col-md-11">&nbsp;<%= @individual.identifier %></div>
  <div class="col-md-1"><em>Name</em></div>
  <div class="col-md-11">&nbsp;<%= @individual.name %></div>
  <div class="col-md-1"><em>eMail</em></div>
  <div class="col-md-11">&nbsp;<%= @individual.email %></div>
  <div class="col-md-12"><br/></div>
  <div class="col-md-1"><em>Licenses</em></div>
  <div class="col-md-11">&nbsp;<%= link_to @individual.licenses.count, greensub_individual_licenses_path(@individual) %></div>
  <div class="col-md-1"><em>Products</em></div>
  <div class="col-md-11">&nbsp;<%= link_to @individual.products.count, greensub_individual_products_path(@individual) %></div>
  <div class="col-md-12"><br/></div>
  <div class="col-md-12">
    <%= link_to_if @individual.update?, 'Edit', edit_greensub_individual_path(@individual) %>
    <span> | </span>
    <%= link_to 'Back', greensub_individuals_path %>
    <span> | </span>
    <%= link_to_if @individual.destroy?, 'Destroy', greensub_individual_path(@individual), method: :delete, data: { confirm: 'Are you sure?' } %>
  </div>
  <div class="col-md-12"><br/></div>
</div>
