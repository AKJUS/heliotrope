<%= simple_form_for(@permit) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :agent_type %>
  </div>

  <div class="form-inputs">
    <%= f.input :agent_id %>
  </div>

  <div class="form-inputs">
    <%= f.input :agent_token %>
  </div>

  <div class="form-inputs">
    <%= f.input :credential_type %>
  </div>

  <div class="form-inputs">
    <%= f.input :credential_id %>
  </div>

  <div class="form-inputs">
    <%= f.input :credential_token %>
  </div>

  <div class="form-inputs">
    <%= f.input :resource_type %>
  </div>

  <div class="form-inputs">
    <%= f.input :resource_id %>
  </div>

  <div class="form-inputs">
    <%= f.input :resource_token %>
  </div>

  <div class="form-inputs">
    <%= f.input :zone_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
